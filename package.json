{
	"name": "support-discord-bot",
	"module": "index.ts",
	"main": "src/index.ts",
	"scripts": {
		"start": "node -r module-alias/register build/index.js",
		"build": "tsc",
		"dev": "concurrently \"tsc -w\" \"nodemon -r module-alias/register build/index.js\"",
		"download:secrets": "doppler secrets download --project=personal-discortify-bot --config=dev --no-file --format=env > .env",
		"commit": "git-cz",
		"release": "commit-and-tag-version"
	},
	"devDependencies": {
		"@biomejs/biome": "1.8.3",
		"@types/bun": "latest",
		"@types/node": "^20.14.2",
		"@types/spotify-api": "^0.0.25",
		"commit-and-tag-version": "12.4.1",
		"commitizen": "4.3.0",
		"concurrently": "^8.2.2",
		"cz-conventional-changelog": "3.3.0",
		"nodemon": "^3.1.3",
		"ts-node": "^10.9.2",
		"typescript": "^5.5.2"
	},
	"engines": {
		"node": ">=20.10.0"
	},
	"dependencies": {
		"@discordjs/rest": "^2.3.0",
		"@prisma/client": "5.15.1",
		"commandkit": "^0.1.10",
		"date-fns": "^3.6.0",
		"discord.js": "^14.15.3",
		"dotenv": "^16.4.5",
		"module-alias": "^2.2.3",
		"numbro": "^2.5.0",
		"picocolors": "^1.1.1",
		"prisma": "^5.15.1",
		"query-string": "^9.0.0",
		"zod": "^3.23.8"
	},
	"_moduleAliases": {
		"@": "build"
	},
	"prisma": {
		"schema": "src/lib/db/schema.prisma"
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	},
	"packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
